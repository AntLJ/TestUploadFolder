<?xml version="1.0" encoding="shift_jis"?>
<CheckReachability>
  <regionlayers>
    <!-- Kiwi標準リージョン設定・フェリー通行不可版 -->
    <regionlayer>
      <unit>R1_unit</unit>
      <overlap>R1_overlap</overlap>
      <whereclause>NAVICLASS_C IN (101, 102, 1, 103, 403, 2, 104, 3, 105, 4, 106, 5, 107, 6, 108)</whereclause>
    </regionlayer>
    <regionlayer>
      <unit>R2_unit</unit>
      <overlap>R2_overlap</overlap>
      <whereclause>NAVICLASS_C IN (101, 102, 1, 103, 403, 2, 104, 3, 105, 106, 107)</whereclause>
    </regionlayer>
    <regionlayer>
      <unit>R3_unit</unit>
      <overlap>R3_overlap</overlap>
      <whereclause>NAVICLASS_C IN (101, 102, 1, 103)</whereclause>
    </regionlayer>
    <regionlayer>
      <whereclause>NOT (NAVICLASS_C = 201 OR WIDEAREA_F = 0)</whereclause>
    </regionlayer>
  </regionlayers>
</CheckReachability>
